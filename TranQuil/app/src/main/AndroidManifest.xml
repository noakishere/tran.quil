<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tranquil">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_app_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_app_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TranQuil">
        <activity android:name=".ActivitiesPage"></activity>
        <activity
            android:name=".Register"
            android:label="@string/title_activity_register"
            android:theme="@style/Theme.TranQuil.NoActionBar" />
        <activity android:name=".MyProfile" />
        <activity android:name=".sos" />
        <activity android:name=".Discover" />
        <activity android:name=".Sleep" />
        <activity android:name=".Meditation" />
        <activity android:name=".MainMenu" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>